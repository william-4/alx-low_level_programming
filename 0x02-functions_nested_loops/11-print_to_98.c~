#include "main.h"

/**
 *print_to_98 - prints numbers from n to 98
 *@n: starting point
 *
 *Return: 0
 */
int print_to_98(int n)
{
	int i;

	for (i = n; i <= 98; i++)
	{
		if (i == 98)
		{
			_putchar(i);
			_putchar('\n');
		}
		else
		{
			_putchar(i);
			_putchar(',');
			_putchar(' ');
		}
	}
}

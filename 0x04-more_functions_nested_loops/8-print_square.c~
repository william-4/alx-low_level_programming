#include <stdio.h>

/**
 *print_square - prints a square
 *@size: the size of the square
 *Return: void
 */
void main(int size)
{
	int i, j;

	size = 5;

	if (size > 0)
	{
		for (i = 0; i < size; i++)
		{
			for (j = 0; j < size; j++)
			{
				putchar('#');
			}
			putchar('\n');
		}
		putchar('\n');
	}
	else
		putchar('\n');
}

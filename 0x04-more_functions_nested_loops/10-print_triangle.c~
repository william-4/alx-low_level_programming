#include <stdio.h>

/**
 *print_triangle - prints a triangle
 *@size: the size of the triangle
 *Return: void
 */
void main(int size)
{
	int i, j, k;

	size = 5;

	if (size > 0)
	{
		for (i = 0; i <= size; i++)
		{
			for (j = size; j <= size; j--)
			{
				putchar(' ');
			}
			putchar('#');
			putchar('\n');
		}
	}
	else
		putchar('\n');
}
